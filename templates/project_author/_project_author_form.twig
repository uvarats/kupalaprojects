<div>
    {{ form_start(form) }}
    <div class="mb-3">
        {{ form_row(form.placeOfWork) }}
    </div>
    <div class="mb-3">
        {{ form_row(form.occupation) }}
    </div>
    <div class="mb-3">
        {{ form_row(form.reserveEmail) }}
    </div>
    {{ form_rest(form) }}
    <button type="submit" class="btn btn-primary">Сохранить</button>
    {{ form_end(form) }}
</div>