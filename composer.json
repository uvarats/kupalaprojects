{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "babdev/pagerfanta-bundle": "^4.4",
        "cakephp/chronos": "^2.3",
        "doctrine/annotations": "^2.0",
        "doctrine/doctrine-bundle": "^2.9",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^3.0",
        "easycorp/easyadmin-bundle": "^4.6",
        "fakerphp/faker": "^1.23",
        "friendsofsymfony/ckeditor-bundle": "^2.5",
        "friendsofsymfony/elastica-bundle": "^6.4",
        "league/flysystem-bundle": "^3.3",
        "nette/utils": "^4.0",
        "openspout/openspout": "^4.24",
        "phpdocumentor/reflection-docblock": "^5.4",
        "phpstan/phpdoc-parser": "^1.19",
        "ramsey/collection": "^2.0",
        "symfony/asset": "7.1.*",
        "symfony/console": "7.1.*",
        "symfony/doctrine-messenger": "7.1.*",
        "symfony/dotenv": "7.1.*",
        "symfony/expression-language": "7.1.*",
        "symfony/flex": "^2",
        "symfony/form": "7.1.*",
        "symfony/framework-bundle": "7.1.*",
        "symfony/http-client": "7.1.*",
        "symfony/intl": "7.1.*",
        "symfony/mailer": "7.1.*",
        "symfony/mailer-send-mailer": "7.1.*",
        "symfony/mime": "7.1.*",
        "symfony/monolog-bundle": "^3.0",
        "symfony/notifier": "7.1.*",
        "symfony/process": "7.1.*",
        "symfony/property-access": "7.1.*",
        "symfony/property-info": "7.1.*",
        "symfony/redis-messenger": "7.1.*",
        "symfony/runtime": "7.1.*",
        "symfony/security-bundle": "7.1.*",
        "symfony/serializer": "7.1.*",
        "symfony/string": "7.1.*",
        "symfony/translation": "7.1.*",
        "symfony/twig-bundle": "7.1.*",
        "symfony/uid": "7.1.*",
        "symfony/ux-autocomplete": "^2.17",
        "symfony/ux-live-component": "^2.17",
        "symfony/ux-turbo": "^2.17",
        "symfony/validator": "7.1.*",
        "symfony/web-link": "7.1.*",
        "symfony/webpack-encore-bundle": "^2.1",
        "symfony/workflow": "7.1.*",
        "symfony/yaml": "7.1.*",
        "twig/cssinliner-extra": "^3.5",
        "twig/extra-bundle": "^3.0",
        "twig/twig": "^3.0"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "ckeditor:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*",
        "twig/twig": "3.10.0"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.1.*"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.40",
        "phpunit/phpunit": "^11.0",
        "symfony/browser-kit": "7.1.*",
        "symfony/css-selector": "7.1.*",
        "symfony/debug-bundle": "7.1.*",
        "symfony/maker-bundle": "^1.59",
        "symfony/phpunit-bridge": "7.1.*",
        "symfony/stopwatch": "7.1.*",
        "symfony/web-profiler-bundle": "7.1.*",
        "symplify/config-transformer": "^12.2",
        "vimeo/psalm": "dev-master"
    }
}
